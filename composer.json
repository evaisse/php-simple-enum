{
    "name": "evaisse/php-simple-enum",
    "description": "A very stupid, basic, enum control struct for php",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Emmanuel VAIÌˆSSE",
            "email": "evaisse@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require": {
        "php": ">=5.6.0",
        "ext-json": "*"
    },
    "require-dev": {
        "satooshi/php-coveralls": "^1.0",
        "phpunit/phpunit": "^5.6.8",
        "doctrine/instantiator": "1.0.*"
    },
    "autoload": {
        "psr-4": {
            "evaisse\\SimplePhpEnum\\Tests\\": "tests/",
            "evaisse\\SimplePhpEnum\\": "src/"
        }
    },
    "scripts": {
        "tests": [
            "php vendor/phpunit/phpunit/phpunit"
        ]
    }
}
